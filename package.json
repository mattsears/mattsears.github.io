{
  "name": "mattsears",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:css": "tailwindcss --postcss  --minify -i ./assets/css/application.css -o ./assets/builds/application.css",
    "build:js": "esbuild ./assets/js/*.* --minify --bundle --outdir=./assets/builds/",
    "build": "yarn build:css && yarn build:js"
  },
  "browserslist": "defaults, not ie <= 11",
  "dependencies": {
    "@hotwired/stimulus": "3.0.1",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.7",
    "animejs": "^3.2.1",
    "esbuild": "^0.15.9",
    "postcss": "^8.4.31",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^14.0.2",
    "postcss-nesting": "^10.1.2",
    "postcss-preset-env": "^7.3.1",
    "stimulus-carousel": "^4.0.0",
    "tailwindcss": "3.0.20",
    "tailwindcss-stimulus-components": "^3.0.4",
    "typed.js": "^2.0.12"
  },
  "devDependencies": {
    "eslint": "8.19.0",
    "eslint-config-next": "12.2.5",
    "prettier": "^2.8.3",
    "prettier-plugin-tailwindcss": "^0.1.13"
  }
}
